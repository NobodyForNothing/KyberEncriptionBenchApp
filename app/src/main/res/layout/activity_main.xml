<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <Button
            android:text="START TEST"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button"
            android:onClick="doTest" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/textView" app:layout_constraintVertical_bias="0.78"
            app:layout_constraintHorizontal_bias="0.916"/>
    <TextView
            android:layout_width="363dp"
            android:layout_height="528dp" android:id="@+id/textView"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.684"/>
    <EditText
            android:layout_width="125dp"
            android:layout_height="47dp"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/editTextNumber"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.426" app:layout_constraintBottom_toTopOf="@+id/textView"
            app:layout_constraintVertical_bias="0.782"/>
    <TextView
            android:text="Establishments"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView2"
            app:layout_constraintEnd_toStartOf="@+id/editTextNumber" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.615"
            app:layout_constraintBottom_toTopOf="@+id/textView"
            android:layout_marginBottom="32dp"/>
    <Spinner
            android:layout_width="409dp"
            android:layout_height="wrap_content" tools:layout_editor_absoluteX="1dp" android:id="@+id/spinner2"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toTopOf="@+id/editTextNumber"
            app:layout_constraintVertical_bias="0.666"/>

</androidx.constraintlayout.widget.ConstraintLayout>